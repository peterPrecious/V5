<!--#include virtual = "V5/Inc/Setup.asp"-->
<!--#include virtual = "V5/Inc/Initialize.asp"-->
<!--#include virtual = "V5/Inc/Db_Phra.asp"-->
<!--#include virtual = "V5/Inc/Db_Cust.asp"-->
<!--#include virtual = "V5/Inc/Db_Prog.asp"-->
<!--#include virtual = "V5/Inc/Db_Mods.asp"-->
<!--#include virtual = "V5/Inc/Certificate.asp"-->

<!--#include virtual = "V5/Inc/RTE.asp"-->
<!--#include file    = "RTE_Routines.asp"-->

<%  
	Dim vUrl, vProgId
  vProgId = Request("vProgId")
	'...get basics (need to refresh)
	sRTEsessionCore svMembNo, Clng(Request("vProgNo").Item), Request("vModsId").Item, Request("vProgId").Item
	'...get objectives (even expired ones)
	sRTEsessionObj2 svMembNo, RTE_ProgNo, RTE_ModsNo
%>

<html>

  <head>
    <title>RTE_ModsStat</title>
    <link href="/V5/Inc/Vubi2.css" type="text/css" rel="stylesheet">
    <script src="/V5/Inc/Functions.js"></script>
    <script src="/V5/Inc/jQuery.js"></script>
    <script>
		  // this saves the original click position so we can return it 
		  // should we need to generate a cert using the openWindow call 
		  // value is passed in parm3 to re-open this window at the original y postion 
		  // not the current y/mouseclick position which will be generated by the click
		  // on the certificate button.
		  var saveY = pageY;
    </script>
    <style>

      .modsStat th { width:30%; }
      .modsStat td { width:70%; }

    </style>


  </head>

  <body>

    <h2><!--webbot bot='PurpleText' PREVIEW='Module Details'--><%=fPhra(001399)%></h2>

    <table class="modsStat">
      <tr>
        <td colspan="2">&nbsp;</td>
      </tr>
      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Module Id'--><%=fPhra(001271)%>:</th>
        <td><%=RTE_ModsId %>&nbsp;&nbsp;&nbsp; <%=RTE_ModsTitle %></td>
      </tr>
      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Module Status'--><%=fPhra(001400)%>:</th>
        <td><%=RTE_Status%> *</td>
      </tr>
      <% If RTE_TimeSpent > 0 Then%>
      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Time Spent in Module'--><%=fPhra(001401)%>:</th>
        <td><%=RTE_TimeSpent%>&nbsp;<!--webbot bot='PurpleText' PREVIEW='minute'--><%=fPhra(001460)%>(s)</td>
      </tr>
      <% End If %>
      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Last Accessed'--><%=fPhra(001402)%>:</th>
        <td><%=fFormatDateTime(RTE_LastDate)%></td>
      </tr>
      <tr>
        <th colspan="2">&nbsp;</th>
      </tr>
    </table>
    
    
  <% If RTE_Attempts > 0 Then %> 

  <br><br><br>
  <h2><!--webbot bot='PurpleText' PREVIEW='Assessment Details'--><%=fPhra(001403)%></h2>

    <table class="modsStat">
      <tr>
        <th>&nbsp;</th>
        <td colspan="2">&nbsp;</td>
        <td rowspan="4" style="width: 50%">

          <fieldset>
            <legend><!--webbot bot='PurpleText' PREVIEW='History'--><%=fPhra(001404)%></legend>
            <table class="table">
              <%

                Dim aScores  : aScores  = Split(RTE_Scores,  "|")
                Dim aDates   : aDates   = Split(RTE_Dates,   "|")
                Dim aExpired : aExpired = Split(RTE_Expired, "|")
                Dim bExpired : bExpired = False

                For i = 0 To Ubound(aScores)
                  Response.Write "<tr><td nowrap>" & fFormatDateTime(aDates(i)) & "</td><td nowrap>" & f5 & aScores(i) & "%</td><td nowrap class='green'>" & f5 & fFormatDateTime(aExpired(i)) & "</span></td></tr>"                      
                  If IsDate(aExpired(i)) Then bExpired = True
                Next

                If bExpired Then Response.Write "<tr><td colspan='3' class='green'>Dates in green are when completed recurring assessments are/were eligible for retake.</td></tr>"

              %>
            </table>
          </fieldset>

        </td>
      </tr>


      <%  If RTE_BestScore > 0 Then %>

      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Last Score'--><%=fPhra(001273)%>:</th>
        <td>
          <%
  				  Response.Write RTE_BestScore
  				%>
        </td>
        <td>
  				<%  
  			    If RTE_Completed And RTE_ModsVuCert Then 
  		  		  vUrl = fCertificateUrl("", "", RTE_BestScore, RTE_CompletedDate, RTE_ModsId, RTE_ModsTitle, "", "", "", RTE_ProgId, "", "", "")
          %>
              <input onclick="openWindow('C', '<%=vUrl%>', saveY)" type="button" value="Certificate" name="B5" class="button070">
          <% 
      		  End If 
          %> 
        </td>
      </tr>
      <% End If  %>


      <%  If RTE_Failed Then %>
      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Assessment Status'--><%=fPhra(001405)%>:</th>
        <td>Failed</td>
        <td>&nbsp;</td>
      </tr>
      <%  End If  %>
      <tr>
        <th><!--webbot bot='PurpleText' PREVIEW='Attempts Used'--><%=fPhra(001406)%>:</th>
        <td><%=RTE_NoAttempts & " of " & fRTE_MaxAttempts(vProgId)%></td>
        <td>&nbsp;</td>
      </tr>
      <tr>
        <th colspan="4">&nbsp;</th>
      </tr>
    </table>

  <% End If %>
    
  <br><br>


  <h2>*  <!--webbot bot='PurpleText' PREVIEW='Status Legend'--><%=fPhra(000248)%></h2>

  <table class="modsStat">
    <tr>
      <th>&nbsp;</th>
      <td>&nbsp;</td>
    </tr>
    <tr>
      <th><!--webbot bot='PurpleText' PREVIEW='Not Started'--><%=fPhra(000192)%>:</th>
      <td><!--webbot bot='PurpleText' PREVIEW='Module has not been accessed.'--><%=fPhra(001407)%></td>
    </tr>
    <tr>
      <th><!--webbot bot='PurpleText' PREVIEW='Reviewed'--><%=fPhra(000229)%>:</th>
      <td><!--webbot bot='PurpleText' PREVIEW='Module has been previously accessed and may be in progress or completed.'--><%=fPhra(001408)%></td>
    </tr>
    <tr>
      <th><!--webbot bot='PurpleText' PREVIEW='Completed'--><%=fPhra(000107)%>:</th>
      <td><!--webbot bot='PurpleText' PREVIEW='Note: some modules may not use or set this status.'--><%=fPhra(001409)%></td>
    </tr>
    <tr>
      <th><!--webbot bot='PurpleText' PREVIEW='Failed'--><%=fPhra(001410)%>:</th>
      <td><!--webbot bot='PurpleText' PREVIEW='An embedded assessment was not passed using the attempts available.'--><%=fPhra(001411)%></td>
    </tr>
    <tr>
      <th colspan="2">&nbsp;</th>
    </tr>
  </table>

  </body>

</html>


